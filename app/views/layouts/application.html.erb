<!DOCTYPE html>
<html>
  <head>
    <title>Readme</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400|PT+Serif|Montserrat:300,400&display=swap" rel="stylesheet"> 
  </head>

  <body>
    <%= render 'layouts/header' %>
    <div class="content">
      <%= yield %>
    </div>
  </body>
</html>
